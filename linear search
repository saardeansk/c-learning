#include<iostream>
#include<vector>
using namespace std;
int main (){
int n=0;
int index=-1;
cout<<"the size of an array"<<endl;
cin>>n;
vector <int> arr(n);
cout<<"enter the elements of array"<<endl;
for(int i = 0;i < n;i++)
{
cin>>arr[i];
}
cout<<" Linear search :"<<endl;
cout<<"the search element are  :"<<endl;
int search;
cin>>search;
for(int i = 0;i < n;i++)
{
  if(search==arr[i])
{
cout<<"the element is found : "<<i<<endl;
index=i;
}
}
if(index==-1)
{cout<<"the element is  not found";
}
else{
cout<<"the element is founded array";

}
return 0;
}
