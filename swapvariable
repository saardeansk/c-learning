#include<iostream>
#include<bits\stdc++.h>
using namespace std;
int main()
{
int a,b;
cout<<"enter the two variables"<<endl;
cin>>a;
cin>>b;
cout<<"the two inputs are before swap  "<<a<<"  "<<b<<endl;
 a=a^b;
 b=a^b;
 a=a^b;
cout<<"the two inputs are after swap  "<<a<<"  "<<b<<endl;
return 0;
}
