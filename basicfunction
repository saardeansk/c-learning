#include<iostream>
using namespace std;
int calculatesum(int a,int b)
{
 int  result=a+b;
 return result;
}

int main()
{
int a;
int b;
int n;
cout<<"enter the no of functions in this:"<<endl;
cin>>n;
for(int i=0;i<n;i++)
{
cin>>a;
cin>>b;

cout<<"sum of two variables a+b:"<<calculatesum(a,b)<<endl;
return 0;

}}
